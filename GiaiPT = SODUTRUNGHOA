#include<bits/stdc++.h>
using namespace std ;
#define ll long long  
ll gcd(ll a, ll b, ll& x, ll& y) {
    if (b == 0) {
        x = 1;
        y = 0;
        return a;
    }
    ll x1, y1;
    ll d = gcd(b, a % b, x1, y1);
    x = y1;
    y = x1 - y1 * (a / b);
    return d;
}
ll inverse(ll a , ll m)
{
	ll x , y ;
	if(gcd(a,m,x,y) == 1)
	{
		return (x%m +m) % m ;
	}else{
		return 0  ;
	}
}
int main()
{
	vector<ll> m = {17,19,11} ;
	vector<ll> a = {16,18,7} ;
	int n = m.size() ;
	//Tinh tich cac modulo cua m 
	ll M = 1 ; 
	ll x = 0 ; 
	for(int i = 0 ; i < m.size() ;i++)
	{
		 M  *= m[i] ;
	}
	for(int i = 0 ; i<n ;i++)
	{
		ll Mi = M / m[i] ;
		
		ll yi = inverse(Mi ,m[i]) ;
		x += ((ll)(a[i]%M) * (Mi % M) * ( yi % M)) % M;
	}
	(x%M + M) % M ;
	cout << "X = " << x << endl ;
	
	
}
