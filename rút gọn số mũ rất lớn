#include<bits/stdc++.h>

using namespace std ;
#define ll long long 

ll phi(ll n)
{
	ll kq = n ; 
	ll can = sqrt(n)  ;
	for(ll p = 2 ; p <= can ; p++)
	{
		if(n % p ==0)
		{
			while(n % p == 0)
			{
				n/=p ;
			}
			kq -=kq/p ;
		}
	}
	if(n > 1) kq -= kq/n ; 
	return kq ;
}
ll chia_du(string n , ll m)
{
	ll res = 0 ; 
	for(char c : n)
	{
		res = res*10 +(c - '0') ;
		res %= m ;
	}
	return res ;
}


int main()
{
     string m ; 
     ll n ; 
     getline(cin,m) ;
     cin >> n ;
     ll p  = phi(n) ;
     ll kq = chia_du(m , p) ;
     cout << kq << endl ;
	 	
	
}
